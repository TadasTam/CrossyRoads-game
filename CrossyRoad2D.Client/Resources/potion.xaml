<Viewbox xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
         xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
         Width="200" Height="200" x:Name="viewbox">
    <Grid Width="200" Height="200">
        <Grid.RenderTransform>
            <TransformGroup>
                <ScaleTransform ScaleX="1" 
                                ScaleY="1" 
                                CenterX="100" 
                                CenterY="100"
                                x:Name="scaleTransform"/>

                <RotateTransform Angle="0" 
                                 CenterX="100" CenterY="100"
                                 x:Name="rotateTransform"/>
            </TransformGroup>
        </Grid.RenderTransform>

        <Ellipse  Width="100" Height="100" Margin="30,30,30,30" Fill="Green" Stroke="Black" StrokeThickness="5"/>
        
        <Ellipse  Width="100" Height="100" Margin="30,30,30,30" Stroke="Black" StrokeThickness="5"/>
        <Ellipse Width="100" Height="100" Margin="30,30,30,30" Stroke="Black" StrokeThickness="5"/>
        <Rectangle Width="30" Height="50" Margin="85,5,85,145" Stroke="black" StrokeThickness="5"/>

    </Grid>
</Viewbox>